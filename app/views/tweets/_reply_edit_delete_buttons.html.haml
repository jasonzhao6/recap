%a.reply-btn.btn{href: tweet_reply_path(tweet), 'data-pjax' => '#content'} Reply
%a.edit-btn.btn{href: edit_tweet_path(tweet), 'data-pjax' => '#content'} Edit
- if deletable
  %a.delete-btn.btn.btn-danger.pull-right{href: tweet_path(tweet), 'data-pjax' => '#content', 'data-method' => 'delete', rel: 'nofollow', 'data-confirm' => 'Are you sure you want to delete?'} Delete