#login-container
  .page-header
    %h1 Recap
  %form#session-form{method: 'post', 'data-remote' => true}
    %input.input-full{name: 'email', placeholder: 'Your email', value: 'test@example.com'}
    %input.input-full{name: 'password', placeholder: 'Type your password', autocomplete: 'off', value: 'test'}
    %button#login.btn.btn-primary.btn-large.input-full{type: 'button'} Login
    %button#sign-up.btn.btn-large.input-full{type: 'button'} Sign up